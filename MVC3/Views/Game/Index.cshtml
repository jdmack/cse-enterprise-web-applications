@model IEnumerable<MVC3.Models.PLGame>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            player1Name
        </th>
        <th>
            player1Race
        </th>
        <th>
            player1Code
        </th>
        <th>
            player1League
        </th>
        <th>
            player2Name
        </th>
        <th>
            player2Race
        </th>
        <th>
            player2Code
        </th>
        <th>
            matchup
        </th>
        <th>
            length
        </th>
        <th>
            time
        </th>
        <th>
            winnerName
        </th>
        <th>
            map
        </th>
        <th>
            spawns
        </th>
        <th>
            size
        </th>
        <th>
            downloadCount
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.player1Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.player1Race)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.player1Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.player1League)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.player2Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.player2Race)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.player2Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.matchup)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.length)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.winnerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.map)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.spawns)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.size)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.downloadCount)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
